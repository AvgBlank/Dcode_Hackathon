generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id          Int      @id
    name        String
    username    String
    about       String?
    accessToken String?  @unique
    twitter     String?
    linkedIn    String?
    mail        String?
    portfolio   String?
    created     DateTime
}
